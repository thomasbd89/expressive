{
  "name": "@siddiqus/expressive",
  "version": "1.1.7",
  "description": "Fast, opinionated, minimalist, and conventional REST API framework for NodeJS, built on ExpressJs.",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat reports/coverage/lcov.info | coveralls",
    "rollup": "rollup -c",
    "build": "babel src --out-dir build",
    "prep:example": "npm run clean && npm run rollup && cp dist/expressive.js example/lib/expressive.js",
    "lint": "eslint src -f node_modules/eslint-detailed-reporter/lib/detailed.js -o reports/lint.html || echo Lint report: reports/lint.html",
    "lintFix": "eslint --fix src || echo Linting done!",
    "dev": "nodemon -w example/index.js --exec node example/index.js"
  },
  "author": "Sabbir Siddiqui <siddiqus@live.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-request-id": "1.4.1",
    "express-validator": "5.3.0",
    "swagger-ui-express": "4.0.1",
    "unirest": "0.6.0"
  },
  "jest": {
    "coverageDirectory": "reports/coverage",
    "collectCoverageFrom": [
      "src"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/runtime-corejs2": "7.4.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "coveralls": "3.0.3",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-google": "0.12.0",
    "eslint-detailed-reporter": "0.8.0",
    "eslint-plugin-react": "7.12.4",
    "jest": "24.7.1",
    "nodemon": "1.19.0",
    "rimraf": "2.6.2",
    "rollup": "1.10.0",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "4.2.3",
    "rollup-plugin-terser": "4.0.4",
    "rollup-plugin-uglify": "6.0.2"
  },
  "homepage": "https://github.com/siddiqus/expressive#readme",
  "bugs": {
    "url": "https://github.com/siddiqus/expressive/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siddiqus/expressive.git"
  },
  "keywords": [
    "express",
    "framework",
    "nodejs",
    "server",
    "javascript",
    "rest api",
    "convention based routing"
  ]
}
